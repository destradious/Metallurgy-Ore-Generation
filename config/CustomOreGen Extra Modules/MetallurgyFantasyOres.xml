    <!--*****************************   Distributions   *************************************
    *   
    *   Currently this Metallurgy Mod Config ONLY uses the Cluster (Stanard) Distribution.
    *
    *     Vanilla Clusters:
    *       A re-creation of the vanilla ore generation using the StandardGen algorithm.
    *
    *************************************************************************************-->
    <IfCondition condition=':= (dimension.generator = "RandomLevelSource") &amp;
                            (dimension.id = 0)'>
        
        <!--**********   Vanilla Deposit Removal   *************-->
        <Substitute name='StandardOreSubstitute' block='stone'>
            <Description> 
                Replace vanilla-generated ore clusters with stone.   
            </Description>
            <Comment>  
                The global option deferredPopulationRange must be large enough to catch all ore clusters (>= 32).
            </Comment>

            <Replaces block='906:0'/>	<!--  Prometheum  -->
            <Replaces block='906:1'/>	<!--  Deep Iron  -->
            <Replaces block='906:2'/>	<!--  Infuscolium  -->
            <Replaces block='906:4'/>	<!--  Oureclase  -->
            <Replaces block='906:5'/>	<!--  Astral Silver  -->
            <Replaces block='906:6'/>	<!--  Carmot  -->
            <Replaces block='906:7'/>	<!--  Mithril  -->
            <Replaces block='906:8'/>	<!--  Rubracium  -->
            <Replaces block='906:11'/>	<!--  Orichalcum  -->
            <Replaces block='906:13'/>	<!--  Adamantine  -->
            <Replaces block='906:14'/>	<!--  Atlarus  -->


        </Substitute>
        
        <!--*******************   Prometheum   ***********************-->
        <ConfigSection>
        
	  <!-- *****  Standard Cluster  ***** -->
            <IfCondition condition=':= if(age, age.prometheumClusters > 0, prometheumDist = "vanillaStdGen")'>    

                <StandardGen name='PrometheumStandard' block='906:0' inherits='PresetStandardGen'> 
                    <Description> Equivalent to vanilla minecraft Cluster distribution. </Description>
                    <DrawWireframe>:=drawWireframes</DrawWireframe>
                    <WireframeColor>0x60101010</WireframeColor>

                    <Setting name='Size' avg=':= 0.75 * prometheumSize * _default_'/> 
                    <Setting name='Frequency' avg=':= 0.25 * prometheumFreq * if(age,age.prometheumClusters,1) * _default_'/>
                    <Setting name='Height' avg=':= 128/128 * dimension.groundLevel' range=':= 128/128 * dimension.groundLevel'/>

  		  <!--1 = 100%  (Normal loads full amount).  0.5 = 50%  (Loads Half as much).  0 = 0%  (None loads in this Biome). -->
                    <BiomeType name='.*' weight='1'/>
                    <Biome name='Hell.*' weight='-1'/>

                </StandardGen>


		<!-- More Distribution Types can go here -->


            </IfCondition>   
            
        </ConfigSection>
        <!-- ********** / Prometheum ********** -->


        <!--*******************   DeepIron   ***********************-->
